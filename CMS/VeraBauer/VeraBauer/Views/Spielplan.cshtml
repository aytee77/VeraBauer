@inherits Umbraco.Web.Mvc.UmbracoTemplatePage<ContentModels.Spielplan>
@using ContentModels = Umbraco.Web.PublishedContentModels;
@{
    Layout = "~/Views/shared/_Master.cshtml";
}

<div class="row">
    <div class="col-sm-6 col-sm-offset-1">
        <h1>@Umbraco.Field("titel")</h1>
    </div>
</div>
<div class="row filter">
    <div class="col-sm-10 col-sm-offset-1">
        <div class="customSelect">
            <select>
                <option>2017</option>
                <option>2016</option>
                <option>2015</option>
                <option>2014</option>
            </select>
        </div>
        <img class="print" src="/img/icon_print.png" />
    </div>
</div>
<section class="auffuehrung">
    <a href="ProgrammDetail.html">
        <div class="row">
            <div class="col-sm-8 col-sm-offset-3">
                <h3>Mensch, Luther!</h3>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-2 col-sm-offset-1 gigDate">
                <p>
                    Mittwoch<br class="hidden-xs" />
                    11. Oktober 2017<br class="hidden-xs" />
                    20 Uhr
                </p>
            </div>
            <div class="col-sm-8">
                <p class="description">Ein Lebensportrait in Briefen, Reflexionen und Musik</p>
                <p>
                    Vera Bauer, Sprecherin – David Goldzycher, Violine.
                    Goethe-Institut Moskau,
                    Leninskij Prospekt 95a,
                    119313 Moskau, Russische Föderation
